    <TextView
        android:id="@+id/textView60"
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NOTIFICATION"
        android:fontFamily="@font/poppins_bold"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView61"
        app:layout_constraintStart_toStartOf="@+id/textView61"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.074" />

    <TextView
        android:id="@+id/textView62"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:gravity="center"
        android:text="Choose whether you want to get a \nLet n' Go notification alert."
        android:textSize="20sp"
        android:textStyle="bold"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.172" />

    <TableLayout
        android:layout_marginTop="30sp"
        android:layout_width="344dp"
        android:layout_height="356dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">
        <TableRow
            android:id="@+id/allNotif"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/allNotifText"
                android:layout_width="244dp"
                android:fontFamily="@font/poppins_bold"
                android:layout_height="match_parent"
                android:layout_column="1"
                android:text="Notifications"
                android:textColor="#000000"
                android:textSize="23sp"
                android:textStyle="bold" />
            <Switch
                android:id="@+id/allNotifSwitch"
                android:layout_width="78dp"
                android:layout_height="match_parent"
                android:layout_column="2"
                tools:ignore="MissingConstraints,UseSwitchCompatOrMaterialXml"
                tools:layout_editor_absoluteX="25dp"
                tools:layout_editor_absoluteY="205dp" />
        </TableRow>
        <TableRow
            android:id="@+id/accActivity"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/accActivityText"
                android:layout_width="261dp"
                android:fontFamily="@font/poppins_bold"
                android:layout_height="match_parent"
                android:layout_column="1"
                android:text="Account Activity"
                android:textColor="#000000"
                android:textSize="23sp"
                android:textStyle="bold" />
            <Switch
                android:id="@+id/accActivitySwitch"
                android:layout_width="78dp"
                android:layout_height="match_parent"
                android:layout_column="2"
                tools:ignore="MissingConstraints,UseSwitchCompatOrMaterialXml"
                tools:layout_editor_absoluteX="25dp"
                tools:layout_editor_absoluteY="205dp" />
        </TableRow>
        <TableRow
            android:id="@+id/guessPolicies"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/guessPoliciesText"
                android:layout_width="261dp"
                android:layout_height="match_parent"
                android:layout_column="1"
                android:fontFamily="@font/poppins_bold"
                android:text="Guest Policies"
                android:textColor="#000000"
                android:textSize="23sp"
                android:textStyle="bold" />
            <Switch
                android:id="@+id/guessPoliciesSwitch"
                android:layout_width="78dp"
                android:layout_height="match_parent"
                android:layout_column="2"
                tools:ignore="MissingConstraints,UseSwitchCompatOrMaterialXml"
                tools:layout_editor_absoluteX="25dp"
                tools:layout_editor_absoluteY="205dp" />
        </TableRow>
        <TableRow
            android:id="@+id/offersUpdates"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/offersUpdatesText"
                android:layout_width="261dp"
                android:layout_height="match_parent"
                android:layout_column="1"
                android:fontFamily="@font/poppins_bold"
                android:text="Offers And Updates"
                android:textColor="#000000"
                android:textSize="23sp"
                android:textStyle="bold" />
            <Switch
                android:id="@+id/offersUpdatesSwitch"
                android:layout_width="78dp"
                android:layout_height="match_parent"
                android:layout_column="2"
                tools:ignore="MissingConstraints,UseSwitchCompatOrMaterialXml"
                tools:layout_editor_absoluteX="25dp"
                tools:layout_editor_absoluteY="205dp" />
        </TableRow>
        <TableRow
            android:id="@+id/reminder"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/reminderText"
                android:layout_width="261dp"
                android:layout_height="match_parent"
                android:layout_column="1"
                android:fontFamily="@font/poppins_bold"
                android:text="Reminders"
                android:textColor="#000000"
                android:textSize="23dp"
                android:textStyle="bold" />
            <Switch
                android:id="@+id/reminderSwitch"
                android:layout_width="78dp"
                android:layout_height="match_parent"
                android:layout_column="2"
                tools:ignore="MissingConstraints,UseSwitchCompatOrMaterialXml"
                tools:layout_editor_absoluteX="25dp"
                tools:layout_editor_absoluteY="205dp" />
        </TableRow>
